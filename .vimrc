syntax on
color wombat  "https://raw.githubusercontent.com/gryf/wombat256grf/master/colors/wombat256grf.vim
filetype on
set autoindent
set colorcolumn=80
set expandtab smarttab
set hlsearch
set ignorecase
set incsearch
set laststatus=2
set number
set ruler
set showmatch
set smartcase
set smartindent
set softtabstop=4
set tabstop=4
set undolevels=500
